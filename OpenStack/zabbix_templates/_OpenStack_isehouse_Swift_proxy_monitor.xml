        <macro>
          <macro>{$SWIFT_HC_ACCOUNT}</macro>
          <value>demo</value>
        </macro>
        <macro>
          <macro>{$SWIFT_HC_KEYSTONE_URL}</macro>
          <value>http://keystone_server:35357/v2.0/</value>
        </macro>
        <macro>
          <macro>{$SWIFT_HC_MAX_CONTAINER}</macro>
          <value>10</value>
